{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"]
      }
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.js" },
    { "src": "/api", "dest": "/index.js" },
    { "src": "/api/(.*)", "dest": "/index.js" }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
